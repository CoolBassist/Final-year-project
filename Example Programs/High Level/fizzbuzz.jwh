# FizzBuzz program in JWhile

# Prints "Fizz" if `i` is divisible by 3 but not 5
# Prints "Buzz" if `i` is divisible by 5 but not 3
# Prints "FizzBuzz" if `i` is divisible by both 3 and 5

for(i := 1; i <= 15; i := i + 1){
	if(i%3 = 0){
		if(i%5 = 0){
			print("FizzBuzz");
		}else{
			print("Fizz");
		}
	}else{
		if(i%5 = 0){
			print("Buzz");
		}else{
			print(i);
		}
	}
}
